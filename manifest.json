{
	"name": "Tab manager",
	"version": "0.1.0",
	"description": "Easily and quickly navigate through your chrome tabs",
	"short_name": "Chrome tab manager",
	"manifest_version": 2,
	"permissions": ["activeTab", "tabs"],

	"background": {
		"scripts": ["./extension/background.js"]
	},

	"commands": {
		"start_application": {
			"suggested_key": {
				"default": "Ctrl+Shift+F",
				"mac": "MacCtrl+Shift+F"
			},
			"description": "Start the application"
		}
	}
}
